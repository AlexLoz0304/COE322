#include <iostream>
#include <vector>
#include <cmath>
using namespace std;

int main () {
  float total = 0;
  vector<float> numbers = {1,2,3,5};

  for (float e: numbers) {
      total += e * e;
 
  }
  
  float L = sqrt(total);

  for (float e: numbers) {
      e = e/L;
      cout << e << "\n";
  }
  
  return 0;
}
